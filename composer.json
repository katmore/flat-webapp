{
    "name": "katmore/flat-dist-webapp",
    "description": "boilerplate flat web application",
    "homepage": "https://github.com/katmore/flat-webapp",
    "authors": [
        {
            "name": "Garrison Koch",
            "email": "garrisonkoch@gmail.com"
        },    
        {
            "name": "Doug Bird",
            "email": "retran@gmail.com"
        }
    ],    
    "require": {
        "php" : ">=7.0.1",
        "katmore/flat" : "dev-master",
        "mongodb/mongodb": "^1.0.0",
        "aws/aws-sdk-php": "^3.19",
        "ext-simplexml" : "*",
        "ext-curl" : "*",
        "ext-imagick" : "*",
        "ext-gmp" : "*",
        "jenssegers/imagehash": "^0.4.0"        
    },
    "autoload": {
        "psr-4": { 
            "SpyDash\\": "src/SpyDash",
            "SpyDashRoute\\": "app/SpyDashRoute",
            "SpyDashWeb\\": "app/SpyDashWeb"
         }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katmore/flat"
        }
    ],
    "license": [
       "MIT",
       "GPL-3.0+"
    ]
}
